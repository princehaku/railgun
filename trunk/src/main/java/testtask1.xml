<?xml version="1.0" encoding="UTF-8"?>
<shell xmlns="http://railgun.techest.net" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://railgun.techest.net shell.xsd">
    <name>oabt</name>
    <description>无极电影抓取</description>
    <baseurl>*</baseurl>
    <reloadtime>10000</reloadtime>
    <stair>
        <fetch>
            <url>http://oabt.org/?page=$[1,3]</url>
        </fetch>
        <parse method="dom">
            <rule>.toplist tbody</rule>
        </parse>
        <handler>net.techest.railgun.test.TestFilter</handler>
    </stair>
</shell>
